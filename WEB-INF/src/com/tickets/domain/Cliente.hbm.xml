<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]>

<hibernate-mapping>

	<class name="com.tickets.domain.Cliente" table="Cliente">

		&hibernate-domain;

		<property name="descripcion" />
		<property name="cedula" />
		<property name="ruc" />
		<property name="nuevo" />
		
		<property name="fechaNacimiento" />
		<property name="edad" />
		<property name="nacionalidad" />
		<property name="residencia" />
		<property name="nivelEducativo" />
		<property name="situacionLaboral" />
		<property name="lugarNacimiento" />
		<property name="etnia" />
		<property name="direccion" />
		<property name="seguroMedico" />
		<property name="estadoCivil" />
		<property name="observacion" />

	</class>

</hibernate-mapping>
